// Jest Snapshot v1, https://goo.gl/fbAQLP

exports[`RequestedReminder Component matches the snapshot 1`] = `
Array [
  <div
    className="body"
  >
    <div
      className="title"
    >
      <h3>
        Reminder Sent
      </h3>
    </div>
    <div
      className="row"
    >
      <div
        className="col-6"
      >
        To
      </div>
      <div
        className="col-6"
      />
    </div>
    <div
      className="row"
    >
      <div
        className="col-6"
      >
        Transfer Amount
      </div>
      <div
        className="col-6"
      >
        <strong
          className="amount"
        />
      </div>
    </div>
  </div>,
  <div
    className="action"
  >
    <div
      className="hstack gap-3"
    >
      <button
        className="zippy-btn zippy-flat d-flex center w-full simple btn btn-primary"
        disabled={false}
        onClick={[Function]}
        type="button"
      >
        Back to Status
      </button>
      <button
        className="zippy-btn d-flex ms-auto center w-full simple btn btn-primary"
        disabled={false}
        onClick={[Function]}
        type="button"
      >
        Send Another
      </button>
    </div>
  </div>,
]
`;
